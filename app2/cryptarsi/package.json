{
  "name": "cryptarsi",
  "version": "0.0.1",
  "license": "GPL",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "npm install && npm install --only=dev && npm run build:cryptarsi",
    "build:cryptarsi": "ng build --prod -o dist-cryptarsi",
    "build:electron:win32": "npm install electron-prebuilt --arch=ia32 --platform=win32",
    "build:electron:win64": "npm install electron-prebuilt --arch=x64 --platform=win32",
    "build:electron:linux64": "npm install electron-prebuilt --arch=x64 --platform=linux",
    "build:electron:osx64": "npm install electron-prebuilt --arch=x64 --platform=darwin",
    "build:osx64": "cross-env npm_config_arch=x64 npm_config_platrofm=darwin WCJS_PLATFORM=osx WCJS_ARCH=x64 WCJS_RUNTIME=electron npm run build:electron:osx64",
    "build:win32": "cross-env npm_config_arch=ia32 npm_config_platrofm=win32 WCJS_PLATFORM=win32 WCJS_ARCH=ia32 WCJS_RUNTIME=electron npm run build:electron:win32",
    "build:linux64": "cross-env npm_config_arch=x64 npm_config_platrofm=linux WCJS_PLATFORM=linux WCJS_ARCH=x64 WCJS_RUNTIME=electron npm run build:electron:linux64",
    "pack:electron:win32": "electron-packager dist-cryptarsi cryptarsi-win32 --platform=win32 --arch=ia32 --out=dist",
    "pack:electron:linux64": "electron-packager dist-cryptarsi cryptarsi-linux64 --platform=linux --arch=x64 --out=dist",
    "pack:electron:osx64": "electron-packager dist-cryptarsi cryptarsi-osx64 --platform=darwin --arch=x64 --out=dist",
    "pack:win32": "npm run build:win32 && npm run pack:electron:win32",
    "pack:linux64": "npm run build:linux64 && npm run pack:electron:linux64",
    "pack:osx64": "npm run build:osx64 && npm run pack:electron:osx64",
    "clean:modules": "rimraf node_modules",
    "clean:cryptarsi": "rimraf dist-cryptarsi",
    "clean": "npm run clean:cryptarsi && npm run clean:modules"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/material": "^2.0.0-alpha.10",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "core-js": "^2.4.1",
    "cross-env": "^3.1.3",
    "crypto-js": "^3.1.8",
    "electron-builder": "^8.6.0",
    "electron-packager": "^8.3.0",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.30",
    "@types/node": "^6.0.42",
    "google-closure-compiler": "^20161024.3.0",
    "angular-cli": "1.0.0-beta.19-3",
    "codelyzer": "1.0.0-beta.1",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.0.3",
    "webdriver-manager": "10.2.5",
    "electron-prebuilt": "^1.3.9"
  }
}
